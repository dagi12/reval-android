    android:id="@+id/activity_check_unique"
    style="@style/AppTheme.DefaultActivityPadding"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="pl.edu.amu.wmi.reval.task.unique.CheckUniqueActivity">

    <include layout="@layout/subject_topic_layout" />

    <Button
        android:id="@+id/check_unique"
        android:layout_width="match_parent"
